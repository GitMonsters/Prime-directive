# RustyWorm Deployment Configuration
# Copy this file to .env and customize

# =============================================================================
# API Keys (optional - for live API observation)
# =============================================================================
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=

# =============================================================================
# Service Ports
# =============================================================================
AGENTRL_PORT=8080
MONGODB_PORT=27017
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000
AGENTCPM_PORT=8000

# =============================================================================
# RL Configuration
# =============================================================================
RL_ALGORITHM=ppo
RL_LEARNING_RATE=0.0003
RL_GAMMA=0.99
RL_BATCH_SIZE=64

# =============================================================================
# Logging
# =============================================================================
LOG_LEVEL=info

# =============================================================================
# Monitoring (if using --monitoring profile)
# =============================================================================
GRAFANA_PASSWORD=admin

